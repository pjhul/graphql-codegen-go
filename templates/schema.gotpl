{{range .Types}}
  {{if eq .Kind "OBJECT"}}
    type {{.Name}} struct {
        {{range .Fields}}{{formatName .Name}} {{formatType .Type}} `json:"{{.Name}}"`{{"\n"}}{{end}}
    }
  {{else if eq .Kind "INTERFACE"}}
    type {{.Name}} struct {
        {{range .Fields}}{{formatName .Name}} {{formatType .Type}} `json:"{{.Name}}"`{{"\n"}}{{end}}
    }
  {{else if eq .Kind "UNION"}}
    type {{.Name}} interface{}
  {{end}}
{{end}}

